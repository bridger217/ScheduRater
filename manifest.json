{
  "name": "ScheduRater",
  "version": "1.0",
  "description": "ScheduRater chrome extension",
  "manifest_version": 2,
  "background": {
    "scripts": ["rmp_query.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://umich.collegescheduler.com/*"
      ],
      "js": [
        "schedurater.js"
      ]
    }
  ],
  "web_accessible_resources":[
    "iframe.html"
  ],
  "permissions": [
    "http://www.ratemyprofessors.com/",
    "https://www.ratemyprofessors.com/"
  ],
  "icons": {
    "128": "icon.png"
  },
  "broswer_action": {
    "default_icon": "icon.png"
  }

}
